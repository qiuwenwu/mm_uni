}
.nav_main .list{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.nav_main .item{
	position: relative;
	margin: 0 0.5rem;
}
.nav_main .item ._i{
	margin-left: 0.25rem;
}
.nav_main .item>._i:hover{
	cursor: pointer;
