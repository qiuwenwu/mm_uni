/* 快捷导航 */
.nav_quick {
	position: relative;
	text-align: center;
}
.nav_quick .dropdown {
	position: absolute;
	z-index:300;
	top: -4rem;
	left: 50%;
	-webkit-transform: translate(-50%) scale(0);
	        transform: translate(-50%) scale(0);
	width: 18rem;
	padding: 0.75rem;
	border: 1px solid;
	border-color: var(--color_border);
	border-radius: 0.5rem;
	-webkit-box-shadow: 0px 2px 10px var(--color_shadow);
	        box-shadow: 0px 2px 10px var(--color_shadow);
	background-color: #fff;
}
/* 箭头 */
.nav_quick .dropdown::before {
	content: "";
	display: inline-block;
	position: absolute;
	top: -10.25px;
	left: 50%;
	width: 0.75rem;
	height: 0.75rem;
	border: 1px solid;
	border-color: var(--color_border) var(--color_border) transparent transparent;
	background-color: #fff;
	-webkit-transform: rotate(-45deg) translate(-50%);
	        transform: rotate(-45deg) translate(-50%);
}
.nav_quick .dropdown.show_drop {
	-webkit-animation: show_drop 0.2s linear forwards;
	        animation: show_drop 0.2s linear forwards;
}
@-webkit-keyframes show_drop {
0% {
		top: 0;
		-webkit-transform: translate(-50%) scale(0);
		        transform: translate(-50%) scale(0);
}
85% {
		top: 2rem;
		-webkit-transform: translate(-50%) scale(1.05);
		        transform: translate(-50%) scale(1.05);
}
100% {
		top: 2rem;
		-webkit-transform: translate(-50%) scale(1);
		        transform: translate(-50%) scale(1);
}
}
@keyframes show_drop {
0% {
		top: 0;
		-webkit-transform: translate(-50%) scale(0);
		        transform: translate(-50%) scale(0);
}
85% {
		top: 2rem;
		-webkit-transform: translate(-50%) scale(1.05);
		        transform: translate(-50%) scale(1.05);
}
100% {
		top: 2rem;
		-webkit-transform: translate(-50%) scale(1);
		        transform: translate(-50%) scale(1);
}
}
.nav_quick .item>.title {
	font-size: 0.875rem;
	font-weight: bold;
	text-align: left;
}
.nav_quick .list_sub {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin: 0 -0.25rem;
}
.nav_quick .item_sub {
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 25%;
	        flex: 0 0 25%;
	padding: 0.25rem;
}
.nav_quick .item_sub>.title {
	display: block;
	font-size: 0.75rem;
	padding: 0.125rem 0;
	text-align: center;
	border-radius: 0.25rem;
	background-color: var(--bg_grey);
}
/* 靠左样式 */
.nav_quick.display_left .dropdown{
	left: 1rem;
	-webkit-transform: scale(0);
	        transform: scale(0);
}
.nav_quick.display_left .dropdown::before{
	left: 1rem;
	-webkit-transform: rotate(-45deg) translate(-50%);
	        transform: rotate(-45deg) translate(-50%);
}
.nav_quick.display_left .dropdown.show_drop {
	-webkit-animation: show_drop_2 0.2s linear forwards;
	        animation: show_drop_2 0.2s linear forwards;
}
@-webkit-keyframes show_drop_2 {
0% {
		top: 0;
		-webkit-transform: scale(0);
		        transform: scale(0);
}
85% {
		top: 2rem;
		-webkit-transform: scale(1.05);
		        transform: scale(1.05);
}
100% {
		top: 2rem;
		-webkit-transform: scale(1);
		        transform: scale(1);
}
}
@keyframes show_drop_2 {
0% {
		top: 0;
		-webkit-transform: scale(0);
		        transform: scale(0);
}
85% {
		top: 2rem;
		-webkit-transform: scale(1.05);
		        transform: scale(1.05);
}
100% {
		top: 2rem;
		-webkit-transform: scale(1);
		        transform: scale(1);
}
}
/* 靠右样式 */
.nav_quick.display_right .dropdown{
	left: initial;
	right: 1rem;
	-webkit-transform: scale(0);
	        transform: scale(0);
}
.nav_quick.display_right .dropdown::before{
	left: initial;
	right: 1rem;
	-webkit-transform: rotate(-45deg) translate(-50%);
	        transform: rotate(-45deg) translate(-50%);
}
.nav_quick.display_right .dropdown.show_drop {
	-webkit-animation: show_drop_2 0.2s linear forwards;
	        animation: show_drop_2 0.2s linear forwards;
}
