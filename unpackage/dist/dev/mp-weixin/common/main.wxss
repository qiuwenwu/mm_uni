/* 反相（反转颜色） 可用于表格或按钮等 */
.inverted {
	-webkit-filter: invert(100%);
	        filter: invert(100%);
}
@-webkit-keyframes wave_animation {
0% {
		top: calc((100% - 0.25rem) / 2);
		left: calc((100% - 0.25rem) / 2);
		width: .25rem;
		height: .25rem;
		opacity: 0.25;
}
50% {
		top: calc((100% - 2rem) / 2);
		left: calc((100% - 2rem) / 2);
		width: 2rem;
		height: 2rem;
		opacity: 1;
}
100% {
		top: calc((100% - 6rem) / 2);
		left: calc((100% - 6rem) / 2);
		width: 6rem;
		height: 6rem;
		opacity: 0;
}
}
@keyframes wave_animation {
0% {
		top: calc((100% - 0.25rem) / 2);
		left: calc((100% - 0.25rem) / 2);
		width: .25rem;
		height: .25rem;
		opacity: 0.25;
}
50% {
		top: calc((100% - 2rem) / 2);
		left: calc((100% - 2rem) / 2);
		width: 2rem;
		height: 2rem;
		opacity: 1;
}
100% {
		top: calc((100% - 6rem) / 2);
		left: calc((100% - 6rem) / 2);
		width: 6rem;
		height: 6rem;
		opacity: 0;
}
}
.wave {
	position: relative;
	overflow: hidden;
}
.wave:active {
	-webkit-filter: saturate(1.5);
	filter: saturate(1.5);
}
.wave:focus::before {
	border-radius: 50%;
	background: rgba(0, 0, 0, .1);
	position: absolute;
	content: "";
	display: block;
	background-attachment: fixed;
	background-position: center center;
	z-index: 2;
	-webkit-animation: wave_animation 1s forwards;
	        animation: wave_animation 1s forwards;
}
.wave:focus::after {
	border-radius: 50%;
	background: rgba(0, 0, 0, .1);
	position: absolute;
	content: "";
	display: block;
	z-index: 2;
	-webkit-animation: wave_animation 1.5s forwards;
	        animation: wave_animation 1.5s forwards;
}

